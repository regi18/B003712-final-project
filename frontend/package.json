{
  "name": "b003712-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "deploy": "npm run build && git add . && git commit -am \"build: updated dist\" && git push && git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "gh-deploy-init": "cd ..\\ && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.15",
    "remove-markdown": "^0.5.0",
    "sass": "^1.63.4",
    "vue": "^3.3.4",
    "vue-markdown-render": "^2.0.1",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.17",
    "@types/prismjs": "^1.26.0",
    "@types/remove-markdown": "^0.3.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^13.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
